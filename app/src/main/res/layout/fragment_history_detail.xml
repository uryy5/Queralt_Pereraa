<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin_land"
    tools:context=".activity.HistoryDetailFragment">

    <TextView
        android:id="@+id/dtl_name"
        android:layout_width="wrap_content"
        android:textColor="@color/WHITE"
        android:layout_height="wrap_content"
        android:text="" />

    <TextView
        android:id="@+id/dtl_date"
        android:layout_width="wrap_content"
        android:textColor="@color/WHITE"
        android:layout_height="wrap_content"
        android:text="" />

    <TextView
        android:id="@+id/dtl_num_cells"
        android:layout_width="wrap_content"
        android:textColor="@color/WHITE"
        android:layout_height="wrap_content"
        android:text="" />

    <TextView
        android:id="@+id/dtl_flag_time"
        android:layout_width="wrap_content"
        android:textColor="@color/WHITE"
        android:layout_height="wrap_content"
        android:text="" />



    <TextView
        android:id="@+id/dtl_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/WHITE"
        android:text="" />

    <TextView
        android:id="@+id/dtl_white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/WHITE"
        android:text="" />

    <TextView
        android:id="@+id/dtl_time_total"
        android:layout_width="wrap_content"
        android:textColor="@color/WHITE"
        android:layout_height="wrap_content"
        android:text="" />

    <TextView
        android:id="@+id/dtl_result"
        android:layout_width="wrap_content"
        android:textColor="@color/WHITE"
        android:layout_height="wrap_content"
        android:text="" />



</LinearLayout>
